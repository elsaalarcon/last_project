<h1>Students#index</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Hobby</th>
      <th>Pokemon</th>
      <th>Lucky number</th>
      <th>Birthday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
  <tr>
      <td><%= student.name %></td>
      <td><%= student.hobby %></td>
      <td><%= student.pokemon %></td>
      <td><%= student.lucky_number %></td>
      <td><%= student.birthday %></td>

      <td><%= link_to 'Destroy', student_path(student[:id]), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
    <% end %>
  </tbody>
</table>

<br>